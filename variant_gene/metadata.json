{
  "@context": {
    "stanza": "http://togostanza.org/resource/stanza#"
  },
  "@id": "variant_gene",
  "stanza:label": "Variant / Gene",
  "stanza:definition": "Display genes on which the variant exists",
  "stanza:parameter": [
    {
      "stanza:key": "tgv_id",
      "stanza:example": "tgv47263127",
      "stanza:description": "TogoVar ID",
      "stanza:required": true
    },
    {
      "stanza:key": "assembly",
      "stanza:example": "GRCh38",
      "stanza:description": "assembly: \"GRCh38\"(default) or \"GRCh37\"",
      "stanza:required": false
    },
    {
      "stanza:key": "ep",
      "stanza:example": "https://togovar.biosciencedbc.jp/sparql",
      "stanza:description": "Endpoint URL",
      "stanza:required": false
    },
    {
      "stanza:key": "api",
      "stanza:example": "https://togovar.biosciencedbc.jp/sparqlist/api",
      "stanza:description": "SPARQList API",
      "stanza:required": false
    },
    {
      "stanza:key": "base_url",
      "stanza:example": "https://togovar.biosciencedbc.jp/stanza",
      "stanza:description": "use absolute path rather than relative path for scripts or stylesheets",
      "stanza:required": false
    }
  ],
  "stanza:usage": "<togostanza-variant_gene tgv_id=\"tgv47263127\"></togostanza-variant_gene>",
