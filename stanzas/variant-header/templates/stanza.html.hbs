{{#with result}}
  <table class="table variant-header">
    <thead>
    <tr>
      {{#each xrefs}}
        <th>{{name}}</th>
      {{/each}}
    </tr>
    </thead>
    <tbody>
    <tr>
      {{#each xrefs}}
        <td>
          {{#each refs}}
            <a href="{{this.url}}">{{this.label}}</a>
          {{/each}}
        </td>
      {{/each}}
    </tr>
    </tbody>
  </table>
{{/with}}

{{#with error}}
  <div class="alert alert-danger">{{message}}</div>
{{/with}}
